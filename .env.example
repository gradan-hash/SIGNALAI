# Signal AI Environment Variables
# Copy this file to .env and add your actual API keys

# Google Gemini API Key (required for all agents)
GOOGLE_API_KEY=your_google_api_key_here

# Optional: Additional API keys for enhanced data sources
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here
FINNHUB_API_KEY=your_finnhub_key_here
NEWS_API_KEY=your_news_api_key_here

# Dashboard Configuration
DASHBOARD_PORT=8080
DASHBOARD_HOST=localhost

# Notification Settings (for pre-market alerts)
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your_email@gmail.com
SMTP_PASSWORD=your_app_password_here

# Webhook URL for Discord/Slack notifications (optional)
DISCORD_WEBHOOK_URL=your_discord_webhook_here
SLACK_WEBHOOK_URL=your_slack_webhook_here